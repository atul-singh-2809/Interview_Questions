import java.util.*;

public class test {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    System.out.println("Enter the value of A:");
    int a = sc.nextInt();
    System.out.println("Enter the value of B:");
    int b = sc.nextInt();
    a = a + b;
    b = a - b;
    a = a - b;
    System.out.println("New value of A:" + a + "\n" + "New Value of B:" + b);
  }
}
