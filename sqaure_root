import java.util.*;

public class test {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int a = sc.nextInt();
    int b = 1;
    for (int i = 2; i <= a; i++) {
      if (i * i <= a) {
        if (i * i == a) {
          b = i;
          break;
        }
        b = i;

      } else
        break;
    }
    System.out.println(b);
  }
}
